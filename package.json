{
	"name": "turbo-print",
	"displayName": "turbo-print",
	"description": "Extension to print something",
	"version": "0.0.1",
	"publisher": "lockonmaram",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:turboPrint.Println",
		"onCommand:turboPrint.MarshalIndent"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "turboPrint.Println",
				"title": "Print line"
			},
			{
				"command": "turboPrint.MarshalIndent",
				"title": "Marshal indent json"
			}
		],
    "keybindings": [
      {
        "command": "turboPrint.MarshalIndent",
        "key": "ctrl+alt+p",
        "mac": "ctrl+alt+p"
      },
      {
        "command": "turboConsoleLog.MarshalIndent",
        "key": "ctrl+alt+m",
        "mac": "ctrl+alt+m"
      }
    ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	},
	"repository": {
    "type": "git",
    "url": "https://github.com/lockonmaram/turbo-print"
  }
}
